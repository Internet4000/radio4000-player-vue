{
  "name": "radio4000-player",
  "description": "A player for Radio4000 channels",
  "author": "Internet4000",
  "version": "0.3.6",
  "homepage": "https://github.com/internet4000/radio4000-player",
  "bugs": "https://github.com/internet4000/radio4000-player/issues",
  "license": "GPL-3.0",
  "scripts": {
    "start": "poi",
    "build": "poi build",
    "test": "start-server-and-test start http://localhost:4002 cy:run",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "prepublishOnly": "npm run build"
  },
  "main": "src/index.js",
  "browser": "dist/radio4000-player.min.js",
  "files": [
    "dist/"
  ],
  "dependencies": {
    "lodash.debounce": "^4.0.8",
    "unfetch": "^3.0.0",
    "vue": "^2.5.13",
    "vue-custom-element": "^2.0.0",
    "youtube-player": "^5.4.0"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^1.1.2",
    "cypress": "^1.4.1",
    "poi": "^9.6.9",
    "start-server-and-test": "^1.0.0",
    "vue-test-utils": "^1.0.0-beta.9"
  },
  "poi": {
    "port": 4002,
    "vendor": false,
    "html": {
      "template": "index.html"
    },
    "extractCSS": false,
    "filename": {
      "js": "radio4000-player.min.js"
    },
    "format": "umd",
    "moduleName": "radio4000Player"
  }
