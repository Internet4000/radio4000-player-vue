{
  "name": "radio4000-player",
  "description": "A player for Radio4000 channels",
  "author": "Internet4000",
  "version": "0.3.2",
  "scripts": {
    "start": "NODE_ENV=development rollup --config --watch",
    "build": "NODE_ENV=production rollup --config && npm run build-minify",
    "build-minify": "babili dist/radio4000-player.js > dist/radio4000-player.min.js",
    "test": "ava",
    "prepublishOnly": "npm run build"
  },
  "main": "dist/radio4000-player.js",
  "browser": "dist/radio4000-player.min.js",
  "files": [
    "dist/"
  ],
  "dependencies": {
    "lodash.debounce": "^4.0.8",
    "unfetch": "^3.0.0",
    "vue": "^2.4.2",
    "vue-custom-element": "^1.1.0",
    "youtube-player": "^5.4.0"
  },
  "devDependencies": {
    "ava": "^0.21.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "babili": "^0.1.4",
    "browser-env": "^3.1.0",
    "require-extension-hooks": "^0.2.0",
    "require-extension-hooks-babel": "^0.1.1",
    "require-extension-hooks-vue": "^0.3.1",
    "rollup": "^0.45.2",
    "rollup-plugin-alias": "^1.3.1",
    "rollup-plugin-babel": "^3.0.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-plugin-vue": "^2.4.0",
    "rollup-watch": "^4.3.1"
  },
  "ava": {
    "require": [
      "./test/helpers/setup.js"
    ]
  }
}
